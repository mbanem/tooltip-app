{
  "name": "tooltip-app",
  "version": "0.0.1",
  "private": true,
  "prettier": {
    "singleQuote": true
  },
  "scripts": {
    "dev": "clear; vite dev",
    "remove": "rm -R node_modules pnpm-lock.yaml; clear; pnpm i; pnpm run dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "clear; svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check . && eslint .",
    "format": "prettier --write ."
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^6.0.0",
    "@sveltejs/kit": "^2.20.8",
    "@sveltejs/vite-plugin-svelte": "6.1.1",
    "@types/eslint": "9.6.1",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "eslint": "^9.25.1",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-svelte": "^3.5.1",
    "postcss": "^8.5.3",
    "postcss-load-config": "^6.0.1",
    "prettier": "^3.5.3",
    "prettier-plugin-svelte": "^3.3.3",
    "sass": "^1.87.0",
    "sass-math": "^1.0.0",
    "svelte-check": "^4.1.7",
    "svelte-preprocess": "^6.0.3",
    "tslib": "^2.8.1",
    "typescript": "^5.8.3",
    "vite": "^7.0.0",
    "vite-plugin-sass-dts": "^1.3.31"
  },
  "type": "module",
  "dependencies": {
    "flowbite": "^3.1.2",
    "gsap": "^3.13.0",
    "sass-embedded": "^1.90.0",
    "svelte": "^5.38.1"
  }
}
